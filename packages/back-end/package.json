{
  "name": "back-end",
  "scripts": {
    "build": "yarn typecheck && yarn clean && webpack-cli --config webpack.config.js --mode=production",
    "clean": "rm -rf dist",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "serve": "node dist/main.js",
    "start": "yarn clean && webpack-cli --config webpack.config.js --watch --mode=development",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.17.3",
    "express-graphql": "0.9.0",
    "graphql": "14.6.0",
    "swapi-graphql-schema": "0.2.4"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/preset-typescript": "7.9.0",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.3",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "babel-loader": "8.1.0",
    "eslint": "6.8.0",
    "fork-ts-checker-webpack-plugin": "4.1.1",
    "nodemon-webpack-plugin": "4.3.1",
    "pnp-webpack-plugin": "1.6.4",
    "prettier": "2.0.1",
    "typescript": "3.8.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11"
  }
}
